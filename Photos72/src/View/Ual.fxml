<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<VBox prefHeight="400.0" prefWidth="640.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Controller.UalController">
  <children>
    <AnchorPane maxHeight="-1.0" maxWidth="-1.0" prefHeight="-1.0" prefWidth="-1.0" VBox.vgrow="ALWAYS">
         <children>
            <ListView fx:id="AlbumView" layoutX="220.0" layoutY="37.0" prefHeight="358.0" prefWidth="200.0" />
            <Text layoutX="255.0" layoutY="26.0" strokeType="OUTSIDE" strokeWidth="0.0" text="User Album List" textAlignment="CENTER" wrappingWidth="129.8837890625">
               <font>
                  <Font name="System Bold" size="16.0" />
               </font>
            </Text>
            <TextField fx:id="NewAlbum" layoutX="33.0" layoutY="69.0" promptText="new album name" />
            <Text layoutX="42.0" layoutY="56.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Create New Album" textAlignment="CENTER" wrappingWidth="142.0" />
            <Button fx:id="Create" layoutX="79.0" layoutY="108.0" mnemonicParsing="false" onAction="#createAlbum" text="+ CREATE" textFill="#0e12da">
               <font>
                  <Font name="Rockwell" size="12.0" />
               </font>
            </Button>
            <Button fx:id="Delete" layoutX="82.0" layoutY="350.0" mnemonicParsing="false" onAction="#deleteAlbum" text="X DELETE" textFill="#dd1010">
               <font>
                  <Font name="Calibri Bold Italic" size="13.0" />
               </font>
            </Button>
            <TextField fx:id="DeleteAlbum" layoutX="42.0" layoutY="309.0" promptText="album for deletion" />
            <Text layoutX="80.0" layoutY="299.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Delete Album" />
            <Button fx:id="OpenAlbum" layoutX="431.0" layoutY="56.0" mnemonicParsing="false" onAction="#openAlbum" text="Open Album" />
            <Button fx:id="TagSearch" layoutX="479.0" layoutY="351.0" mnemonicParsing="false" text="Tag Search" />
            <Text layoutX="431.0" layoutY="48.0" strokeType="OUTSIDE" strokeWidth="0.0" text="select album from list" />
            <TextField fx:id="SearchType" layoutX="438.0" layoutY="269.0" promptText="type=value" />
            <TextField fx:id="SearchValue" layoutX="439.0" layoutY="309.0" promptText="AND/OR type=value" />
            <Button fx:id="DateSearch" layoutX="476.0" layoutY="187.0" mnemonicParsing="false" text="Date Search" />
            <TextField fx:id="SearchDate" layoutX="438.0" layoutY="143.0" promptText="enter a date range" />
            <Text layoutX="445.0" layoutY="136.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Search For Photos By Date" />
            <Text layoutX="445.0" layoutY="257.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Search For Photos By Tag(s)" />
            <Button layoutX="15.0" layoutY="9.0" mnemonicParsing="false" text="logout" />
            <Button layoutX="545.0" layoutY="6.0" mnemonicParsing="false" text="save and quit" />
            <TextField fx:id="CurrentAlbum" layoutX="38.0" layoutY="169.0" promptText="Current Album Name" />
            <TextField fx:id="RenameAlbum" layoutX="38.0" layoutY="204.0" promptText="New Album Name" />
            <Text layoutX="75.0" layoutY="160.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Rename Album" />
            <Button fx:id="Rename" layoutX="80.0" layoutY="240.0" mnemonicParsing="false" onAction="#renameAlbum" text="- RENAME" textFill="#31b543" />
         </children>
    </AnchorPane>
  </children>
</VBox>
